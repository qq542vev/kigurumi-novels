		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">萌味屋</foaf:nick>
			<dcterms:identifier rdf:datatype="&xsd;string">xriOMPkqNA</dcterms:identifier>
