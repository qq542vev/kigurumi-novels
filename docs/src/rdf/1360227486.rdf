	<types:BoardPost rdf:about="&post;43">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">43</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick xml:lang="ja">名無しさん@着ぐるみすと</foaf:nick>
		</dcterms:creator>
		<sioc:delivered_at rdf:datatype="&dcterms;W3CDTF">2013-02-11T01:29:35+09:00</sioc:delivered_at>
		<sioc:content xml:lang="ja">おいその
は、もう来ないでいい
駄作ばっかなのに、荒らす</sioc:content>
