	<types:BoardPost rdf:about="&post;219">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">219</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">玩具</foaf:nick>
