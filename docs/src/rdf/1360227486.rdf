	<types:BoardPost rdf:about="&post;106">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">106</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick xml:lang="ja">名無しさん@着ぐるみすと</foaf:nick>
		</dcterms:creator>
		<sioc:delivered_at rdf:datatype="&dcterms;W3CDTF">2013-02-26T01:09:39+09:00</sioc:delivered_at>
		<sioc:content xml:lang="ja">この後６０秒ＣＭが入ります。</sioc:content>
