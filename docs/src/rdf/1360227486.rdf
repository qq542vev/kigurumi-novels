		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">宅配便   休暇 序</foaf:nick>
			<dcterms:identifier rdf:datatype="&xsd;string">3rZQiXcf5A</dcterms:identifier>
