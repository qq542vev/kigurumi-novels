		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">続・とある物産店 （理沙すとーりー）</foaf:nick>
			<dcterms:identifier rdf:datatype="&xsd;string">1pLg/IIvYk</dcterms:identifier>
