				<rdfs:label rdf:datatype="http://www.w3.org/2001/XML_Schema#positiveInteger">295</rdfs:label>
				<dcterms:creator rdf:parseType="Resource">
					<foaf:nick rdf:datatype="http://www.w3.org/2001/XML_Schema#string">45/52 </foaf:nick>
