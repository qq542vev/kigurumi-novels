				<dcterms:creator rdf:parseType="Resource">
					<foaf:nick rdf:datatype="http://www.w3.org/2001/XML_Schema#string">42/52 </foaf:nick>
					<dcterms:identifier rdf:datatype="http://www.w3.org/2001/XML_Schema#string">XksB4AwhxU</dcterms:identifier>
				</dcterms:creator>
