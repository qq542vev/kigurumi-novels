			<types:BoardPost rdf:about="https://web.archive.org/web/20090323011551/http://jbbs.livedoor.jp/anime/846/storage/1184654919.html#426">
				<rdfs:label rdf:datatype="http://www.w3.org/2001/XML_Schema#positiveInteger">426</rdfs:label>
