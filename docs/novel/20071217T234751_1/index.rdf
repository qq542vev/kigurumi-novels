				<sioc:content xml:lang="ja">食い物を粗末にするな&gt;&gt;436
ttp://www1.plala.or.jp/niwatori/recipe/book004/re061.htm</sioc:content>
				<dcterms:relation rdf:resource="https://web.archive.org/web/20090323011551/http://jbbs.livedoor.jp/anime/846/storage/1184654919.html#436"/>
				<schema:creativeWorkStatus xml:lang="en">Published</schema:creativeWorkStatus>
