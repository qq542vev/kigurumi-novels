		<dcterms:alternative xml:lang="ja">深夜の公園での出会い</dcterms:alternative>
		<schema:creativeWorkStatus xml:lang="en">Incomplete</schema:creativeWorkStatus>
		<dcterms:hasPart rdf:parseType="Collection">
			<types:BoardPost rdf:about="https://web.archive.org/web/20090322230543/http://jbbs.livedoor.jp/anime/846/storage/1122950720.html#803">
