			<types:BoardPost rdf:about="https://web.archive.org/web/20150111082648/http://kigurumi.net63.net/kako/1247437212.html#292">
				<rdfs:label rdf:datatype="http://www.w3.org/2001/XML_Schema#positiveInteger">292</rdfs:label>
				<dcterms:creator rdf:parseType="Resource">
