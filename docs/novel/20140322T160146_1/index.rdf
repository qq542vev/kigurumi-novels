					<foaf:nick rdf:datatype="http://www.w3.org/2001/XML_Schema#string">シャチ女</foaf:nick>
					<dcterms:identifier rdf:datatype="http://www.w3.org/2001/XML_Schema#string">wQSXY643Ks</dcterms:identifier>
