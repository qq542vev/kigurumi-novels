	<types:BoardPost rdf:about="&post;744">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">744</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">457　―　着ぐるみの囚人服　―　</foaf:nick>
			<dcterms:identifier rdf:datatype="&xsd;string">sAEsqgy2Kg</dcterms:identifier>
