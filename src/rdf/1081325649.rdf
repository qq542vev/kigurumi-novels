	<types:BoardPost rdf:about="&post;933">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">933</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">26/44 </foaf:nick>
			<dcterms:identifier rdf:datatype="&xsd;string">XksB4AwhxU</dcterms:identifier>
