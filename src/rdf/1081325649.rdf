	<types:BoardPost rdf:about="&post;5">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">5</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">人形屋</foaf:nick>
