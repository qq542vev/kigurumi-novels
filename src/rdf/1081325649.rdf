	<types:BoardPost rdf:about="&post;467">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">467</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">恋愛小説家</foaf:nick>
