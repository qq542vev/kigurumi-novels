	<types:BoardPost rdf:about="&post;440">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">440</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">Avan小説サイド</foaf:nick>
