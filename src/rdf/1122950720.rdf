	<types:BoardPost rdf:about="&post;526">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">526</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick xml:lang="ja">名無しさん@着ぐるみすと</foaf:nick>
