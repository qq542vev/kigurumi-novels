	<types:BoardPost rdf:about="&post;688">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">688</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick xml:lang="ja">名無しさん@着ぐるみすと</foaf:nick>
		</dcterms:creator>
