	<types:BoardPost rdf:about="&post;401">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">401</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string"/>
			<dcterms:identifier rdf:datatype="&xsd;string">oDpAfMeajc</dcterms:identifier>
