	<types:BoardPost rdf:about="&post;434">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">434</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">夢幻征四郎</foaf:nick>
