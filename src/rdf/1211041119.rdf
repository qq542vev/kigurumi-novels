	<types:BoardPost rdf:about="&post;240">
		<rdfs:label rdf:datatype="&xsd;positiveInteger">240</rdfs:label>
		<dcterms:creator rdf:parseType="Resource">
			<foaf:nick rdf:datatype="&xsd;string">ピカ星</foaf:nick>
