	<foaf:Document rdf:nodeID="main">
		<dcterms:title xml:lang="ja">習い事</dcterms:title>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2005-05-28T05:08:51+09:00</dcterms:dateSubmitted>
