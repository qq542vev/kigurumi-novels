	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">ぬいぐるみの奥の世界</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2003-11-27T06:42+09:00</dcterms:dateSubmitted>
		<dcterms:modified rdf:datatype="&dcterms;W3CDTF">2003-12-24T03:19+09:00</dcterms:modified>
