	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">あざらしの皮</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2004-02-16T16:41+09:00</dcterms:dateSubmitted>
		<dcterms:modified rdf:datatype="&dcterms;W3CDTF">2004-02-16T17:58+09:00</dcterms:modified>
