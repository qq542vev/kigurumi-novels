	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">再燃する着ぐるみ熱</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2007-12-12T02:48:27+09:00</dcterms:dateSubmitted>
		<dcterms:modified rdf:datatype="&dcterms;W3CDTF">2007-12-12T03:18:31+09:00</dcterms:modified>
