	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">大胆アイドルくるみちゃん</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2004-05-01T22:47+09:00</dcterms:dateSubmitted>
