	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">救急救命</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2009-01-12T23:32:02+09:00</dcterms:dateSubmitted>
		<dcterms:modified rdf:datatype="&dcterms;W3CDTF">2009-01-12T23:41:56+09:00</dcterms:modified>
