			<types:BoardPost rdf:about="https://web.archive.org/web/20090803104804/http://jbbs.livedoor.jp/anime/846/storage/1081325649.html#487">
				<rdfs:label rdf:datatype="http://www.w3.org/2001/XML_Schema#positiveInteger">487</rdfs:label>
				<dcterms:creator rdf:parseType="Resource">
					<foaf:nick rdf:datatype="http://www.w3.org/2001/XML_Schema#string">恋愛小説家</foaf:nick>
