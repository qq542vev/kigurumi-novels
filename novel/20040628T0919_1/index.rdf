	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">７００万円の依頼</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2004-06-28T09:19+09:00</dcterms:dateSubmitted>
		<dcterms:modified rdf:datatype="&dcterms;W3CDTF">2004-07-03T18:31+09:00</dcterms:modified>
