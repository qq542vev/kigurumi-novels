					<foaf:nick rdf:datatype="http://www.w3.org/2001/XML_Schema#string">人形の洋館　第四章</foaf:nick>
					<dcterms:identifier rdf:datatype="http://www.w3.org/2001/XML_Schema#string">qDggTAordc</dcterms:identifier>
