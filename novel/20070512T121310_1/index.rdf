	<foaf:Document rdf:nodeID="main">
		<dcterms:alternative xml:lang="ja">暑さと孤独の中で</dcterms:alternative>
		<dcterms:dateSubmitted rdf:datatype="&dcterms;W3CDTF">2007-05-12T12:13:10+09:00</dcterms:dateSubmitted>
